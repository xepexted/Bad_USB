REM Title: OSX_Prank
REM Authors: xepexted|texabyte
REM Description: Open a series of apps and display messages
REM Target: macOS 11.0+
REM
DELAY 500
GUI SPACE
DELAY 500
STRING terminal
ENTER
DELAY 500
STRING open -a "Safari"
ENTER
DELAY 1000
STRING Hello! This is your friendly neighborhood rubber duck!
DELAY 1000
GUI SPACE
DELAY 500
STRING terminal
ENTER
DELAY 500
STRING open -a "Calculator"
ENTER
DELAY 500
GUI SPACE
DELAY 500
STRING terminal
ENTER
DELAY 500
STRING osascript -e 'tell application "Calculator" to display dialog "Time to crunch some numbers!"'
ENTER
DELAY 1000
STRING osascript -e 'display notification "Click on the Calculator window" with title "Notification"'
ENTER
DELAY 500
GUI SPACE
DELAY 500
STRING terminal
ENTER
DELAY 1000
STRING osascript -e 'tell application "Calculator" to quit'
ENTER
DELAY 1000
STRING osascript -e 'tell application "Safari" to quit'
ENTER
